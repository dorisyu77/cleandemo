			script(src='http://html5shim.googlecode.com/svn/trunk/html5.js')
	body
		#wrapper
			// start header
			header
				.navbar.navbar-fixed-top
					.container
						.navbar-header
							button.navbar-toggle(type='button', data-toggle='collapse', data-target='.navbar-collapse')
								span.icon-bar
								span.icon-bar
								span.icon-bar
							a.navbar-brand(href='index.html')
								img(src='img/logo.png', alt='logo')
						.navbar-collapse.collapse
							ul.nav.navbar-nav
								li
									a(href='index.html') 預定清潔服務
								li
									a(href='about.html') 公司介紹
								li
									a(href='services.html') 服務項目
								li.active
									a(href='contact.html') 聯絡我們
				.navbar-wrapper
					.container-fluid
						nav.navbar.navbar-fixed-top
							.container
								.navbar-header
									button.navbar-toggle.collapsed(type='button', data-toggle='collapse', data-target='#navbar', aria-expanded='false', aria-controls='navbar')
										span.sr-only Toggle navigation
										span.icon-bar
										span.icon-bar
										span.icon-bar
									a.navbar-brand(href='index.html')
										img(src='img/logo.png', alt='logo')
								#navbar.navbar-collapse.collapse
									ul.nav.navbar-nav
										li
											a(href='index.html') 預定清潔服務
										li
											a(href='about.html') 公司介紹
										li
											a(href='services.html') 服務項目
										li.active
											a(href='contact.html') 聯絡我們
			// end header
			section#inner-headline
				.container
					.row
						.col-lg-12
							h2.pageTitle 聯絡我們
			section#content
				.container
					.row
						.col-md-12
							div
								h2 Contact
								| TEL：0935-970700 / Email：jentao@jentao.com.tw
							br
					.row
						.col-md-6.col-sm-offset-3
							#contactSuccess.alert.alert-success.hidden
								strong 成功!
								|  您的訊息已被送出給我們
							#contactError.alert.alert-error.hidden
								strong 錯誤!
								|  您的訊息並未被送出
							.contact-form
								// NOTE: Update your email Id in "contact_me.php" file in order to receive emails from your contact form
								form#contactForm(name='sentMessage', novalidate='')
									h3 聯絡我們
									.control-group
										.controls
											input#name.form-control(type='text', placeholder='Full Name', required='', data-validation-required-message='Please enter your name')
											p.help-block
									.control-group
										.controls
											input#email.form-control(type='email', placeholder='Email', required='', data-validation-required-message='Please enter your email')
									.control-group
										.controls
											textarea#message.form-control(rows='10', cols='100', placeholder='Message', required='', data-validation-required-message='Please enter your message', minlength='5', data-validation-minlength-message='Min 5 characters', maxlength='999', style='resize:none')
									#success  
									// For success/fail messages
									button.btn.btn-success.wow.fadeInUp.pull-right(type='submit') 送出
									br
			footer
				.container
					.row
						.col-sm-12
							.copyright
								p TEL：0935-970700 / Email：jentao@jentao.com.tw
								p 人道事業有限公司 版權所有 Copyright © 2017
		a.scrollup(href='#')
			i.fa.fa-angle-up.active
